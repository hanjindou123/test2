<h1>Listing subscribes</h1>
<%= session[:s0] %>
<table>
  <thead>
    <tr>
      <th>Subscribe instrument sn</th>
      <th>Subscribe user name</th>
      <th>Subscribe submit datetime</th>
      <th>8:30-9:00</th>
      <th>9:00-9:30</th>
      <th>9:30-10:00</th>
      <th>10:30-10:30</th>
      <th>10:30-11:00</th>
      <th>13:30-14:00</th>
      <th>14:30-14:30</th>
      <th>14:30-15:00</th>
      <th>15:30-15:30</th>
      <th>15:30-16:00</th>
      <th>16:30-16:30</th>
      <th>16:30-17:00</th>

      <th>Subscribe cost</th>
      <th>Subscribe state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscribes1.each do |subscribe| %>
      <tr>
        <td><%= subscribe.subscribe_instrument_sn %></td>
        <td><%= subscribe.subscribe_user_name %></td>
        <td><%= subscribe.subscribe_submit_datetime %></td>
        <td><%= subscribe.subscribe_use_slot0 %></td>
        <td><%= subscribe.subscribe_use_slot1 %></td>
        <td><%= subscribe.subscribe_use_slot2 %></td>
        <td><%= subscribe.subscribe_use_slot3 %></td>
        <td><%= subscribe.subscribe_use_slot4 %></td>
        <td><%= subscribe.subscribe_use_slot5 %></td>
        <td><%= subscribe.subscribe_use_slot6 %></td>
        <td><%= subscribe.subscribe_use_slot7 %></td>
        <td><%= subscribe.subscribe_use_slot8 %></td>
        <td><%= subscribe.subscribe_use_slot9 %></td>
        <td><%= subscribe.subscribe_use_slot10 %></td>
        <td><%= subscribe.subscribe_use_slot11 %></td>
        <td><%= subscribe.subscribe_cost %></td>
        <td><%= subscribe.subscribe_state %></td>
        <td><%= link_to 'Show', subscribe %></td>
        <td><%= link_to 'Pay', subscribe%></td>
        <td><%= link_to 'Edit', edit_subscribe_path(subscribe) %></td>
        <td><%= link_to 'Destroy', subscribe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

